# Environment Variables Template
# Copy this file to .env and update with your actual credentials
# NEVER commit the .env file to version control!

# Tableau Server Configuration
TABLEAU_SERVER_URL=https://tableau.yourcompany.com
TABLEAU_SITE_ID=your-site-id
TABLEAU_USERNAME=your-username
TABLEAU_PASSWORD=your-password

# Or use Personal Access Token (recommended)
# TABLEAU_TOKEN_NAME=your-token-name
# TABLEAU_TOKEN_VALUE=your-token-value

# Optional: Default Project
TABLEAU_DEFAULT_PROJECT=Production

# Optional: Logging Level
LOG_LEVEL=INFO
